{
  "name": "solid-graphql-example-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "codegen": "run-s codegen:*",
    "codegen:client": "graphql-codegen --config codegen.ts",
    "codegen:graphql": "cp ../backend/schema.graphql ./schema/schema.graphql",
    "codegen:router": "vite build --config vite.router.ts",
    "coverage": "vitest --coverage",
    "dev": "vite --host 0.0.0.0",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint . --ext .js --ext .ts --ext .tsx --ext .jsx --ext .cjs --fix",
    "format": "run-s format:*",
    "format:prettier": "prettier --write .",
    "format:sort-package-json": "sort-package-json",
    "generate-graph": "depcruise src --include-only '^src' --output-type dot | dot -T svg > dependency-graph.svg",
    "lint": "run-s lint:*",
    "lint:dep": "depcruise --config .dependency-cruiser.cjs .",
    "lint:eslint": "eslint . --ext .js --ext .ts --ext .tsx --ext .jsx --ext .cjs",
    "lint:nginx": "nginx-linter --include ./docker/nginx.conf",
    "lint:prettier": "prettier --check .",
    "lint:sort-package-json": "sort-package-json --check",
    "lint:tsc": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "ui:add": "./tools/add-ui.sh",
    "watch": "tsx ./tools/watch.ts"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.3",
    "@felte/solid": "^1.2.13",
    "@felte/validator-zod": "^1.0.17",
    "@generouted/solid-router": "^1.19.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@kobalte/core": "^0.12.6",
    "@mdx-js/rollup": "^3.0.1",
    "@solid-primitives/clipboard": "^1.5.10",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.1",
    "@tanker/file-reader": "^4.2.0",
    "@thisbeyond/solid-dnd": "^0.7.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "graphql": "^16.9.0",
    "graphql-sse": "^2.5.3",
    "i18next": "^23.12.2",
    "initials": "^3.1.2",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.18",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.2",
    "@solidjs/testing-library": "^0.8.9",
    "@tailwindcss/typography": "^0.5.13",
    "@testing-library/jest-dom": "^6.4.8",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "dependency-cruiser": "^16.3.9",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-solid": "^0.14.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "jsdom": "^24.1.1",
    "nginx-linter": "^0.7.0",
    "npm-run-all2": "^6.2.2",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "remark-emoji": "^4.0.1",
    "solid-devtools": "^0.30.1",
    "sort-package-json": "^2.10.0",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vite": "^5.3.4",
    "vite-plugin-solid": "^2.10.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.5.0"
}
